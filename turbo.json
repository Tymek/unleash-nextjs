{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "UNLEASH_BASE_URL",
    "UNLEASH_FRONTEND_API_TOKEN",
    "UNLEASH_API_TOKEN",
    "UNLEASH_APP_NAME",
    "NEXT_PUBLIC_UNLEASH_BASE_URL",
    "NEXT_PUBLIC_UNLEASH_FRONTEND_API_TOKEN",
    "NEXT_PUBLIC_UNLEASH_APP_NAME"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
